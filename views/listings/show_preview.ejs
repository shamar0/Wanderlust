
<% layout("/layouts/boilerplate") %>
<link rel="stylesheet" href="/css/add-res.css">

    <div id="carouselExample" class="show-preview  mt-4 carousel slide">
        <div class="carousel-inner">
          <div class="carousel-item active">
            <img class="img-fluid" style="height:32rem; width: 70rem; border-radius: 1.8rem;" src="<%=listing.image[0].url%>"  alt="...">
          </div>
    
         <%for(let i=1; i<listing.image.length; i++){%>
            <div class="carousel-item">
              <img class="img-fluid" style="height:32rem; width: 70rem; border-radius: 1.8rem;" src="<%=listing.image[i].url%>"  alt="...">
            </div>
         <%}%>
         <button class="carousel-control-prev d-block w-0 btn-preview" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next d-block w-60 btn-preview" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="visually-hidden">Next</span>
        </button>
        </div>
        
      </div>
      <br> <br>


